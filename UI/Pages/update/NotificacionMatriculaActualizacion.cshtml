@page
@using API.Controllers;
@using APPCORE;
@using APPCORE.Security
@using DataBaseModel;
@{
    string? identify = HttpContext.Session?.GetString("sessionKey") ?? "";
    if (!SecurityController.HavePermission(identify, Permissions.SEND_MESSAGE))
    {
        Response.Redirect("/Security/Login");
        return;
    }
}
<script type="module" >
    import {NotificacionMatriculaActualizacion} from "/update/NotificacionMatriculaActualizacion.js";
    window.onload = ()=> {
        Main.append(new NotificacionMatriculaActualizacion());
    }
</script>
<div id="Main" class="app-container"></div>
