@page
@using API.Controllers;
@using APPCORE;
@using APPCORE.Security
@using DataBaseModel;
@{
    string? identify = HttpContext.Session?.GetString("seassonKey") ?? "";
    if (!SecurityController.HavePermission(identify, Permissions.ADMINISTRAR_USUARIOS))
    {
        Response.Redirect("/Security/Login");
        return;
    }
}
<script type="module" >
    import {ProfileManager} from "/Gestion_Perfil/ProfileManager.js";
    window.onload = ()=> {
        Main.append(new ProfileManager());
    }
</script>
<div id="Main" class="app-container"></div>
