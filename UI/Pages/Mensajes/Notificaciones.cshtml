@page
@using API.Controllers
@using CAPA_DATOS.Security
@using DataBaseModel
@{
    string? identify = HttpContext?.Session.GetString("seassonKey");
    var pageConfig = Config.pageConfig();
    if (!SecurityController.HavePermission(identify, Permissions.NOTIFICACIONES_READER))
    {
        Response.Redirect("./");
        return;
    }
}
<script type="module">
    import {NotificacionesView} from "/Gestion_Mensajes/NotificacionesView.js";
    window.onload = ()=> {
        Main.append(new NotificacionesView());
    } 
</script>
<div id="Main"></div>