@using API.Controllers
@using APPCORE.Security
@using DataBaseModel
@{
    string? identify = HttpContext?.Session.GetString("seassonKey");
    var pageConfig = Config.pageConfig();
    if (!SecurityController.HavePermission(identify, Permissions.NOTIFICACIONES_READER))
    {
        Response.Redirect("./");
        return;
    }
}
<script type="module">
    import {NotificacionesManagerView} from "/Gestion_Mensajes/NotificacionesManagerView.js";
    window.onload = ()=> {
        MainN.append(new NotificacionesManagerView());
    } 
</script>
<div id="MainN"></div>