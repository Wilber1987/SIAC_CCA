@page
@using API.Controllers
@using CAPA_DATOS.Security
@using DataBaseModel
@{
    string? identify = HttpContext?.Session.GetString("seassonKey");
    var pageConfig = Config.pageConfig();
    if (!SecurityController.HavePermission(identify, Permissions.REPORTE_ACCESS))
    {
        Response.Redirect("/");
        return;
    }
}
<script type="module">
    import {ReporteRecorridosView} from "/Reportes/ReporteRecorridosView.mjs";
    window.onload = ()=> {
        Main.append(new ReporteRecorridosView({initialize: true}));
    } 
</script>
<div id="Main"></div>